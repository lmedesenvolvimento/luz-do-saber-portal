    "normalize.css": "^8.0.1",
    "qs": "^6.7.0",
    "string-mask": "^0.3.0",
    "uniqid": "^5.0.3",
    "v-viewer": "^1.4.1",
    "vue": "^2.6.10",
    "vue-content-loader": "^0.2.1",
    "vue-drag-drop": "^1.1.4",
    "vue-electron": "^1.0.6",
